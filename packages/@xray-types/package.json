    "@rollup/plugin-commonjs": "*",
    "@rollup/plugin-node-resolve": "*",
    "@rollup/plugin-typescript": "*",
    "@types/node": "*",
    "rimraf": "*",
    "rollup-plugin-peer-deps-external": "*",
    "rollup-plugin-terser": "*",
    "ts-node": "*",
    "tsconfig-paths": "*",
    "typescript": "*"
  },
  "gitHead": "ff297101a4dd1311beb7184ac535c61ecc3a2b87"
}
